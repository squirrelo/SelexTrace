    clusters = cluster_seqs(args.i, args.sim, folderout=args.o, gapopen='10.0/*TI', gapext='10.0')
    #remove tiny clusters
    topop = []
    countarray = []
    for cluster in clusters:
        totalseqs = 0
        for seq in clusters[cluster]:
            totalseqs += int(seq[0].split("_")[1])
        if totalseqs < 100:
            topop.append(cluster)
        else:
            countarray.append((cluster,count))
    for remove in topop:
        clusters.pop(remove)
    countarray.sort(reverse=True, lambda c: c[1])

    for c in countarray:
        print c[0] + "\t" + str(c[1])